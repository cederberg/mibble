        ::= { mrdTunBoundaryEntry 3 }
 
mrdTunBoundMaskLength OBJECT-TYPE
        SYNTAX        INTEGER
	ACCESS    read-write
	STATUS	mandatory
        DESCRIPTION
            "This value gives the mask length of the scoped address. "
        ::= { mrdTunBoundaryEntry 4 }
 
mrdTunAdminState OBJECT-TYPE
        SYNTAX        INTEGER {
			       delete(1),
