        INDEX         { mrdTunLocalAddress,
			mrdTunRemoteAddress,
			mrdTunScopedAddress }
        ::= { mrdTunBoundaryTable 1 }
 
MrdTunBoundaryEntry ::=
        SEQUENCE {
                mrdTunLocalAddress           	IpAddress,
                mrdTunRemoteAddress           	IpAddress,
