mrdGroupIgmpv1Flg OBJECT-TYPE
        SYNTAX        INTEGER {
		isIgmpV1(1),
		isNotIgmpV1(2)
		}
	ACCESS    read-only
	STATUS	mandatory
        DESCRIPTION
            "This flag indicates whether or not this interface is running IGMP V1."
        ::= { mrdGroupEntry 17 }

