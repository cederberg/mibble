							uni30(1),
							uni31(2),
							uniIisp(3)
								}
				ACCESS	read-write
				STATUS	mandatory
				DESCRIPTION
					"UNI Version used in the chassis"
                    ::= { xylnatmInterfaceConfEntry 22}

                        xylnatmInterfaceCharging        OBJECT-TYPE
                                SYNTAX  INTEGER { enabled(1), disabled(2) }
                                ACCESS  read-write
                                STATUS  mandatory
                                DESCRIPTION
                                        "States whether Charging Detail Records
                                        must be generated for SVCs and/or PVCs
                                        on this interface.
                                        Charging behavior for individual PVCCs
                                        or PVPCs can be set via the corresponding
                                        cross-connect tables."
                    ::= { xylnatmInterfaceConfEntry 23 }
 
                        xylnatmInterfaceCongestStrategy OBJECT-TYPE
                                SYNTAX  INTEGER { acceptCalls(1), refuseCalls(2) }
                                ACCESS  read-write
                                STATUS  mandatory
                                DESCRIPTION
                                        "States what happens if the switch cannot
                                         store Charging Detail Records due to
                                         congestion. The value 'acceptCalls' allows new
                                         SVCs to be established, while 'refuseCalls'
                                         disallows new SVCs.
                                         PVCs are not impacted by this parameter."
                    ::= { xylnatmInterfaceConfEntry 24 }

