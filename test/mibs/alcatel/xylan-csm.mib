                         STATUS   mandatory
                         DESCRIPTION
                             "This object indicates if RTVBR shaping is enabled 
			      on the port."
		         DEFVAL   { true }

                         ::= { xylnatmTrafficShapingEntry 4 }


		      xylnatmTrafficShapingNRTVBR OBJECT-TYPE
                         SYNTAX   INTEGER { false(0), true(1) }
                         ACCESS   read-write
                         STATUS   mandatory
                         DESCRIPTION
                             "This object indicates if NRTVBR shaping is enabled 
			      on the port."
		         DEFVAL   { true }

                         ::= { xylnatmTrafficShapingEntry 5 }


		      xylnatmTrafficShapingABR OBJECT-TYPE
                         SYNTAX   INTEGER { false(0), true(1) }
                         ACCESS   read-write
                         STATUS   mandatory
                         DESCRIPTION
                             "This object indicates if ABR shaping is enabled 
			      on the port."
		         DEFVAL   { true }

                         ::= { xylnatmTrafficShapingEntry 6 }


		      xylnatmTrafficShapingUBR OBJECT-TYPE
                         SYNTAX   INTEGER { false(0), true(1) }
                         ACCESS   read-write
                         STATUS   mandatory
                         DESCRIPTION
                             "This object indicates if UBR shaping is enabled 
			      on the port."
		         DEFVAL   { true }

                         ::= { xylnatmTrafficShapingEntry 7 }


