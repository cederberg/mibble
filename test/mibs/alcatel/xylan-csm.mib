             		        xylnatmTrafficShapingNRTVBR      INTEGER,
             		        xylnatmTrafficShapingABR         INTEGER,
             		        xylnatmTrafficShapingUBR         INTEGER
                               }


                    xylnatmTrafficShapingSlotIndex OBJECT-TYPE
