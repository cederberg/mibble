                         ::= { xylnatmTrafficShapingEntry 1 }


	            xylnatmTrafficShapingPortIndex OBJECT-TYPE
                         SYNTAX   INTEGER (1..10)
                         ACCESS   read-only
                         STATUS   mandatory
                         DESCRIPTION
                             "The port on which traffic shaping circuitry resides.
                              A CSM-ABT module can have two traffic shaping ports."

                         ::= { xylnatmTrafficShapingEntry 2 }


                      xylnatmTrafficShapingCBR OBJECT-TYPE
