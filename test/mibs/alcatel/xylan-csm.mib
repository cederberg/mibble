                    ::= { xylnatmTrafficShapingGroup 1 }

               xylnatmTrafficShapingEntry   OBJECT-TYPE
                    SYNTAX   XylnatmTrafficShapingEntry
                    ACCESS   not-accessible
                    STATUS   mandatory
                    DESCRIPTION
            	        "An entry in the Traffic Shaping ATC flag table."

                    INDEX   { xylnatmTrafficShapingSlotIndex, xylnatmTrafficShapingPortIndex }
                    ::= { xylnatmTrafficShapingTable 1 }

                        XylnatmTrafficShapingEntry ::=
