             Metric    ::= INTEGER (0..'FFFF'h)
             BigMetric ::= INTEGER (0..'FFFFFF'h)

     --  Boolean Values

             TruthVal ::= INTEGER { false (0), true (1)  }

     --  RtrStatus Values

