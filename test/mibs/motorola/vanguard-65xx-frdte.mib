                    Counter,
                cdx6500frdtesRNRFramesIn
                    Counter,
                cdx6500frdtesRNRFramesOut
                    Counter,
                cdx6500frdtesRRFramesIn
                    Counter,
                cdx6500frdtesRRFramesOut
                    Counter,
                cdx6500frdtesREJFramesIn
                    Counter,
                cdx6500frdtesREJFramesOut
                    Counter,
                cdx6500frdtesDataPktsIn
                    Counter,
                cdx6500frdtesDataPktsOut
                    Counter,
                cdx6500frdtesResetStats
                    INTEGER,
                cdx6500frdtesBoot
                    INTEGER,
                cdx6500frdtesDisable
                    INTEGER,
                cdx6500frdtesEnable
                    INTEGER,
                cdx6500frdtesStatsStationNumber
                    INTEGER
                }

        cdx6500frdtesStatsPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies a specific port in the network diagram."
        ::= { cdx6500SPSTFRDTEStationEntry 1 }

        cdx6500frdtesStatsDLCI OBJECT-TYPE
            SYNTAX INTEGER (16..1007)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Data Link Connection Identifier is the unique identifier for 
                 the station on the FRI port and must match the DLCI configured
                 on the Frame Relay networking node."
        ::= { cdx6500SPSTFRDTEStationEntry 2 }

        cdx6500frdtesUtilizationIn OBJECT-TYPE
            SYNTAX INTEGER (0..100)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Indicates the quantity of user bytes sent down the link 
                 expressed as a percentage of the maximum number of bytes that 
                 could have been sent."
        ::= { cdx6500SPSTFRDTEStationEntry 3 }

        cdx6500frdtesUtilizationOut OBJECT-TYPE
            SYNTAX INTEGER (0..100)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Indicates the quantity of user bytes sent down the link 
                 expressed as a percentage of the maximum number of bytes that 
                 could have been sent."
        ::= { cdx6500SPSTFRDTEStationEntry 4 }

        cdx6500frdtesMaxSVCCount OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Maximum number of SVCs available in the network."
        ::= { cdx6500SPSTFRDTEStationEntry 5 }

        cdx6500frdtesCurrentSVCCount OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Current number of SVCs currently in use."
        ::= { cdx6500SPSTFRDTEStationEntry 6 }

        cdx6500frdtesCharInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of characters received since last node boot or 
                 statistics reset."
        ::= { cdx6500SPSTFRDTEStationEntry 7 }

        cdx6500frdtesCharOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of characters transmitted since last node boot or 
                 statistics reset."
        ::= { cdx6500SPSTFRDTEStationEntry 8 }

        cdx6500frdtesCharsInPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of characters received per second."
        ::= { cdx6500SPSTFRDTEStationEntry 9 }

        cdx6500frdtesCharsOutPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of characters transmitted per second."
        ::= { cdx6500SPSTFRDTEStationEntry 10 }

        cdx6500frdtesPktInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
