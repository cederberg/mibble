        cdx6500frdtesCfgPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "FR DTE Port number."
        ::= { cdx6500SPCTFRDTEStationEntry 1 }

        cdx6500frdtesCfgDLCI OBJECT-TYPE
            SYNTAX INTEGER (16..1007)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The Data Link Connection Identifier (dlci) is the unique 
                 identifier for the station on the FRI Port. Its valid range 
                 is 16 - 1007. It must match the DLCI configured on the FR 
                 Networking Node."
        ::= { cdx6500SPCTFRDTEStationEntry 2 }

        cdx6500frdtesStationType OBJECT-TYPE
            SYNTAX INTEGER {
                    annexG     (1),
                    bypass     (2),
                    voiceRelay (3)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Identifies the type of station being configured.
                 annexG      - This station uses X.25 signaling for layer 3
                 bypass      - This station type bypasses layer 3. There is
                               no signaling for SVC's on this station.
                 voiceRelay  - Not used in the current version."
        ::= { cdx6500SPCTFRDTEStationEntry 3 }

        cdx6500frdtesCommInfoRate OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter (CIR) specifies the rate (in bits-per-second) 
                 that the Frame Relay network agrees to transfer information 
                 under congested conditions for the FRI Station.
                 This value is used to throttle data on the FRI station for 
                 congestion control purposes. The CIR cannot be greater than 
                 the link speed."
        ::= { cdx6500SPCTFRDTEStationEntry 4 }

        cdx6500frdtesCommBurstSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter (BC) specifies the maximum amount of data (in 
                 bits) that the Frame Relay network agrees to transfer over a 
                 time interval T = Committed Burst Size / Committed Information
                 Rate. Must be > 1/20th of the Committed Information Rate.
                 This is used for congestion control purposes."
        ::= { cdx6500SPCTFRDTEStationEntry 5 }

        cdx6500frdtesTransDelay OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is the expected one-way delay through the 
                 network. This is specified in milliseconds."
        ::= { cdx6500SPCTFRDTEStationEntry 6 }

        cdx6500frdtesControlledMode OBJECT-TYPE
            SYNTAX INTEGER {
                    normal    (1),
                    disable   (2),
                    congested (3),
                    limit     (4),
                    nc        (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of congestion control:
                 normal     - congestion control is normal
                 disable    - never enter Congested State (CTRL)
                 congested  - never enter a Non-Congested State (UCTRL)
                 limit      - congestion control is normal but limits the
                              data rate to no lower than CIR.
                 nc         - Skipped during configuration"
        ::= { cdx6500SPCTFRDTEStationEntry 7 }

        cdx6500frdtesLinkAddress OBJECT-TYPE
            SYNTAX  INTEGER {
                     dte (1),
                     dce (2),
                     nc  (100)
                     }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The logical link address must be set to complementary values 
                 in this FRI station and its corresponding remote FRI station:
                 dte - station to have logical DTE address (A)
                 dce - station to have logical DCE address (B)
                 nc  - Skipped during configuration."
        ::= { cdx6500SPCTFRDTEStationEntry 8 }

        cdx6500frdtesPVCChannels OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used for Permanent Virtual Circuits.
                 The total number of PVC and SVC channels on a station (logical
                 link) should be kept as small as possible and consistent with 
                 needs. PVC connections must be configured in the PVC Table."
        ::= { cdx6500SPCTFRDTEStationEntry 9 }

        cdx6500frdtesStartingPVC OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the Permanent 
                 Virtual Circuits on this FRI station. Not used if the number 
                 of PVCs = 0."
        ::= { cdx6500SPCTFRDTEStationEntry 10 }

        cdx6500frdtesSVCChannels OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used in Switched Virtual Circuits.
                 The total number of PVC and SVC channels on a station (logical
                 link) should be kept as small as possible and consistent with 
                 needs."
        ::= { cdx6500SPCTFRDTEStationEntry 11 }

        cdx6500frdtesStartingSVC OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the Switched 
                 Virtual Circuits on this station (logical link). Not used if 
                 the number of SVCs = 0."
        ::= { cdx6500SPCTFRDTEStationEntry 12 }

        cdx6500frdtesInitialFrame OBJECT-TYPE
            SYNTAX INTEGER {
                    sabm (1),
                    disc (2),
                    none (3),
                    nc   (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specify the initial frame required by other end for (logical)
                 link startup:
                 none - do nothing (other end initiates link-up)
                 sabm - send SABM
                 disc - send DISC, then SABM
                 nc   - Skipped during configuration"
        ::= { cdx6500SPCTFRDTEStationEntry 13 }

        cdx6500frdtesRetryTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "T1 link retry timer value in tenths of a second (e.g. 30 = 3.0
                 seconds)."
        ::= { cdx6500SPCTFRDTEStationEntry 14 }

        cdx6500frdtesPollTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This sets the time an idle link is probed for assurance of 
                 connection to the remote FRI station.
                 0 - disable the timer
                 X - X is in tenths of a second (e.g. 40 = 4.0 seconds) and
                     must be set greater than T1."
        ::= { cdx6500SPCTFRDTEStationEntry 15 }

        cdx6500frdtesTries OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The maximum number of attempts allowed to complete a 
                 transmission."
        ::= { cdx6500SPCTFRDTEStationEntry 16 }

        cdx6500frdtesFrameWinSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Frame level window size must be set to the same value
                 at both this FRI station and the remote FRI station."
        ::= { cdx6500SPCTFRDTEStationEntry 17 }

        cdx6500frdtesPacketWinSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Packet level window size must be set to the same value 
                 at both this FRI station and the remote FRI station."
        ::= { cdx6500SPCTFRDTEStationEntry 18 }

        cdx6500frdtesMaxPacketSize OBJECT-TYPE
            SYNTAX INTEGER {
                    psize32   (6),
                    psize64   (7),
                    psize128  (8),
                    psize256  (9),
                    psize512  (10),
                    psize1024 (11),
                    nc        (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the maximum X.25 packet size used on the station 
                 (logical link).
                 For proper operation, this must be set to the same value 
                 at both this FRI station and the remote FRI station.
                 nc - Skipped during configuration. "
        ::= { cdx6500SPCTFRDTEStationEntry 19 }

        cdx6500frdtesUpperQueue OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the maximum number of data packets a channel on this 
                 station (logical link) will queue for transmission before 
                 invoking flow control to the attached channel"
        ::= { cdx6500SPCTFRDTEStationEntry 20 }

        cdx6500frdtesLowerQueue OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the number of data packets a channel on this FRI
                 station will have queued for transmission when it releases
                 flow control to the attached channel"
        ::= { cdx6500SPCTFRDTEStationEntry 21 }

        cdx6500frdtesRestartTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Restart-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Restart request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 22 }

        cdx6500frdtesResetTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Reset-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Reset request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 23 }

        cdx6500frdtesCallTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Call-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the call is cleared."
        ::= { cdx6500SPCTFRDTEStationEntry 24 }

        cdx6500frdtesClearTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Clear-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Clear request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 25 }

        cdx6500frdtesX25Options OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "Select X.25 options on this station (logical link) as follows:
                 NONE  - no option specified
                 CUG   - check closed user group; otherwise, CUG passes 
                         transparently
                 CAUSE - pass cause, diagnostic, code in outbound packets
                 HOLD  - hold calls over link restart
                 INL   - inter-node link. Link goes to another 6500 device
                 DELAY - Enable Delay and Path Trace on this link (link must be
                         connected to Rel. 4.xx node)
                 Any combination of above specified by summing 
                 (e.g. CUG+CAUSE+. . .).
                 NOTE: Please use cdx6500frdtesStnX25Options, which has been 
                 added to replace this object."
        ::= { cdx6500SPCTFRDTEStationEntry 26 }

        cdx6500frdtesRCDestination OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..255))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "All calls entering this station will be routed to the 
                 destination specified in this parameter, irrespective of 
                 route selection table entries. For example, to route all 
                 calls to X.25 port 3, use X25-3.
                 To route calls to FRI port 2, station 4, use FRI-2S4. Blank 
                 this field to disable this function."
        ::= { cdx6500SPCTFRDTEStationEntry 27 }

        cdx6500frdtesCUG OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..23))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The station may be a member of up to 8 different Closed User 
                 Groups (CUGs). Each CUG membership must be a two digit number
                 and separated by a comma. 
                 (e.g. 12,34,56,09,02,03 ).
                 Two hyphens following each oher indicates no CUG Membership
                 00-99 - CUG Membership"
        ::= { cdx6500SPCTFRDTEStationEntry 28 }

        cdx6500frdtesBillingRecords OBJECT-TYPE
            SYNTAX INTEGER {
                    off (1),
                    on  (2),
                    nc  (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This controls whether billing (accounting) records will be 
                 created for calls on this FRI station.
                 off - Billing records will not be created.
                 on  - Billing records will be created.
                 nc  - Skipped during configuration."
        ::= { cdx6500SPCTFRDTEStationEntry 29 }

        cdx6500frdtesCfgStationNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the logical link on the line.
                 This is the value of Sxx for routing purposes."
        ::= { cdx6500SPCTFRDTEStationEntry 30 }

        cdx6500frdtesStnX25Options OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..24))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Select X.25 options on this station (logical link) as follows:
                 NONE  - no option specified
                 CUG   - check closed user group; otherwise, CUG passes 
                         transparently
                 CAUSE - pass cause, diagnostic, code in outbound packets
                 HOLD  - hold calls over link restart
                 INL   - inter-node link. Link goes to another 6500 device
                 DELAY - Enable Delay and Path Trace on this link (link must be
                         connected to Rel. 4.xx node)
                 Any combination of above specified by summing 
                 (e.g. CUG+CAUSE+. . .).
                 NOTE: This object has been added to replace 
                 cdx6500frdtesX25Options."
        ::= { cdx6500SPCTFRDTEStationEntry 31 }

        cdx6500frdtesStnFrameSegmenter OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Determine if this station will support the Frame Segmenter
                 Header.The Frame Segmenter should be enabled if the frame
                 relay port is configured to pass voice traffic.
                 disabled - Frame Segmenter disabled.
                 enabled  - Frame Segmenter enabled.
                 nc       - Skipped during configuration."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRDTEStationEntry 32 }


        cdx6500frdtesStnVoiceSVCChannels OBJECT-TYPE
            SYNTAX INTEGER (0..15)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of logical voice channels used in Switched Virtual
                 Circuits.The total number of voice and data channels on a
                 station (logical link) should be kept as small as possible
                 and consistent with needs."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRDTEStationEntry 33 }

        cdx6500frdtesStnVoiceCongCtrlMode OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of voice congestion 
                 control:
                 enabled  - Station is forced into congestion when voice 
                            calls are detected.
                 disabled - Station operates normally
                 Use this parameter in voice applications, when the frame 
                 relay network can have more than 140ms of data buffered. 
                 One typical application is asymmetrical speeds. When this 
                 parameter is enabled, and voice is detected, delta T is 
                 forced to 50 ms and the BC is forced to 1/20 CIR."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRDTEStationEntry 34 }

        cdx6500frdtesStnPeakUtilization OBJECT-TYPE
            SYNTAX INTEGER (0..240)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "The monitoring interval size specifies the time period
                  to use when monitoring peak data link/CIR (Committed
                  Information Rate) utilization.

                  0 - Disables this monitoring feature.
                  1 to 240 - The number of 64 second increments to use for the
                  monitoring interval time period.
                  For example: a value of 30 specifies a time duration
                  equal to 30 * 64 seconds = 1920 seconds or 32 minutes."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRDTEStationEntry 35 }

        cdx6500frdtesStnMaxInboundQueue OBJECT-TYPE
            SYNTAX INTEGER (100..2500)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This specifies the maximum number of frames from the
                 network which may be queued in a BYPASS station. A
                 smaller value may ve choosen for applications which
                 experience objectable delays, if due to excessive
                 growth of the BYPASS inbound queue."
--            DEFVAL { 2500 }
        ::= { cdx6500SPCTFRDTEStationEntry 36 }

        cdx6500frdtesStnAnnexGRateReduction OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc (100)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter determines whether Annex G stations will
                 invoke implicit congestion control when a link error is
                 detected, reducing their transmitted information rate
                 to 1/4 CIR. The rate will be gradually increased in the
                 normal way as non-BECN frames are received."
--            DEFVAL { enabled }
        ::= { cdx6500SPCTFRDTEStationEntry 37 }

        -- The Frame Relay DTE Station Statistics Table
        -- This table contains Statistics paramaters
        -- for the Frame Relay DTE Station table.

        cdx6500SPSTFRDTEStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500SPSTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the statistics parameters relevant
                 to the Frame Relay DTE Station (DLCI) table."
        ::= { cdx6500PSTStationProtocolGroup 2 }

        cdx6500SPSTFRDTEStationEntry OBJECT-TYPE
            SYNTAX Cdx6500SPSTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of Frame Relay DTE station (DLCI)
                 Statistics parameters."
            INDEX { cdx6500frdtesStatsPortNum, cdx6500frdtesStatsStationNumber }
        ::= { cdx6500SPSTFRDTEStationTable 1 }

