        cdx6500frdcesResetStats OBJECT-TYPE
            SYNTAX INTEGER {
                    reset   (1),
                    noReset (2)
                    }
            ACCESS write-only
            STATUS obsolete
            DESCRIPTION
                "reset   : Resets the specified Frame Relay station statistics.
                 noReset : Do not reset the specified Frame Relay station
                           statistics."
        ::= { cdx6500SPSTFRDCEStationEntry 30 }

        cdx6500frdcesBoot OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2)
                    }
            ACCESS write-only
            STATUS obsolete
            DESCRIPTION
                "boot   : Boots the specified Frame Relay station.
                 noBoot : Do not boot the specified Frame Relay station."
        ::= { cdx6500SPSTFRDCEStationEntry 31 }

        cdx6500frdcesDisable OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)
                    }
            ACCESS write-only
            STATUS obsolete
            DESCRIPTION
                "disable   : Disables the specified Frame Relay station.
                 noDisable : Do not disable the specified Frame Relay station."
        ::= { cdx6500SPSTFRDCEStationEntry 32 }

